<%- include('partials/header'); -%>
<section class="contact-clean" style="background: url(&quot;assets/img/bgcov.svg&quot;) center / cover no-repeat;">
    <form class="contact-form" method="post">
        <h2 class="text-center" style="color: #007580;">Contact Us</h2>
        <div class="mb-3"><input class="form-control" type="text" name="name" placeholder="Name"></div>
        <div class="mb-3"><input class="form-control" type="email" name="email" placeholder="Email"></div>
        <div class="mb-3"><textarea class="form-control" name="message" placeholder="Message" rows="14"></textarea></div>
        <div class="mb-3"><button class="btn btn-primary" id="submit-btn" type="submit">send </button></div>
    </form>
</section>
<%- include('partials/footer'); -%>
